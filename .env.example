# GitHub OAuth App credentials
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# Random 32+ character string for AES-GCM encryption of OAuth state/sessions
ENCRYPTION_SECRET=

# Optional: GitHub personal access token for unauthenticated reads
# If not set, unauthenticated users won't be able to see comments
GITHUB_TOKEN=

# Public: base URL of this Agora instance (used by the component for OAuth)
NEXT_PUBLIC_AGORA_HOST=http://localhost:3000
